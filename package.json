{
  "name": "slopes",
  "version": "1.0.6",
  "description": "AVA Platform JS Library",
  "main": "dist/slopes.js",
  "types": "typings/slopes/src/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "prepublish": "npx tsc",
    "test": "jest",
    "test-watch": "jest --watch",
    "docshtml": "npx typedoc --plugin typedoc-plugin-external-module-name --mode file src",
    "docsmd": "npx typedoc --plugin typedoc-plugin-markdown,typedoc-plugin-external-module-name --theme markdown --out docsMD src",
    "docs": "npm run docshtml && npm run docsmd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ava-labs/slopes.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ava-labs/slopes/issues"
  },
  "homepage": "https://github.com/ava-labs/slopes#readme",
  "devDependencies": {
    "@types/bn.js": "^4.11.6",
    "@types/create-hash": "^1.2.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.24",
    "clean-webpack-plugin": "^3.0.0",
    "expose-loader": "^0.7.5",
    "git-revision-webpack-plugin": "^3.0.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-mock-axios": "^3.2.0",
    "terser-webpack-plugin": "^2.3.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.15.6",
    "typedoc-ava-theme": "^1.0.6",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-markdown": "^2.2.14",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "engines": {
    "node": ">=12.13.1",
    "npm": ">=6.12.1"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "bn.js": "^4.11.8",
    "buffer": "^5.4.3",
    "create-hash": "^1.2.0",
    "elliptic": "^6.5.2",
    "secp256k1": "^4.0.0",
    "store2": "^2.10.0"
  }
